{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "sunsuriadf01"
		},
		"SunsuriaDM_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SunsuriaDM'"
		},
		"REST_Defects_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://engine.pegasuswork.com/powerbi_api"
		},
		"REST_VP_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://vp.pegasuswork.com/api/v4/en/powerbi-api"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "sunsuriadf01_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"SunsuriaDM_connectionString": {
						"value": "[parameters('SunsuriaDM_connectionString')]"
					},
					"REST_Defects_properties_typeProperties_url": {
						"value": "[parameters('REST_Defects_properties_typeProperties_url')]"
					},
					"REST_VP_properties_typeProperties_url": {
						"value": "[parameters('REST_VP_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}